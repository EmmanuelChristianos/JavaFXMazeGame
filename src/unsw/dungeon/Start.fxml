<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="startBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleStartBtn" prefHeight="60.0" prefWidth="196.0" text="Start">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="mapSelect" prefWidth="150.0">
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
      </ChoiceBox>
      <Label fx:id="mapError" text="Error: Select map" textFill="RED" visible="false" />
   </children>
</VBox>
